# .env.example
# REQUIRED
RIOT_API_KEY=replace_me

# Collector controls
PLATFORMS=NA1,EUW1,KR
SEED_SUMMONERS=1500
MATCHES_PER=20

# Leave PATCH blank in collector to keep all patches
PATCH=

# Ranked Standard only (1100). Set empty to collect all queues.
QUEUE_FILTER=1100

# Only keep matches from the last N days (0 = no filter)
MAX_AGE_DAYS=14

# Aggregator thresholds
MIN_PICKS=200
MIN_PICKS_ITEM_COMBO=50
